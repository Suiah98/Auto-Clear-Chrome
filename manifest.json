{
	"name": "Auto Clear Chrome",
	"version": "1.1.0",
	"description": "Limpia Chrome Automáticamente (historial , descargas y caché) cada día a la hora que quieras",
	"homepage_url": "https://github.com/Suian98/Auto-Clear-Chrome",
	"manifest_version": 2,
	"permissions": [
		"browsingData",
		"storage",
		"notifications"
	],
	"icons": {
		"128": "icon.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icon.png"
	}
}